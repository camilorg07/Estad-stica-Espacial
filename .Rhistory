p_h_cin1 = 0.05
p_h_c23 = 0.01
p_mort_base = 0.005
p_c1_h = 0.03
p_c1_c23 = 0.08
p_c1_mort = 0.01
p_c23_h = 0.01
p_c23_mort = 0.015
p_ce_ca = 0.10
p_ce_mort = 0.10
p_ca_mort = 0.20
